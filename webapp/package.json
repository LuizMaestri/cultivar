{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-albus": "^2.0.0",
    "react-dom": "^16.4.1",
    "react-icons": "^3.0.1",
    "react-input-mask": "^2.0.2",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.3.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start --env-file=dev.env",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build --env-file=.env",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow"
  },
  "devDependencies": {
    "flow-bin": "^0.77.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react-scripts": "1.1.4"
  }
}
